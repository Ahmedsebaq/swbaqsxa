                        left: ${Math.random() * 100}%;
                        top: ${Math.random() * 100}%;
                        width: ${Math.random() * 10 + 5}px;
                        height: ${Math.random() * 10 + 5}px;
                        background: ${colors[Math.floor(Math.random() * colors.length)]};
                        border-radius: 50%;
                    `;
                    document.body.appendChild(confetti);
                    setTimeout(() => confetti.remove(), 3000);
                }, i * 50);
            }
        }

        // ============ ุฑุจุท ุฒุฑ ุงูุชุงูู ============
        document.getElementById('nextBtn').addEventListener('click', nextQuestion);

        // ============ ุจุฏุก ุงูุชุทุจูู ============
        displayQuestion();
    </script>

    <!-- ุตูุช "ูููู" ุนูุฏ ุถุบุท ุงูุฃุฒุฑุงุฑ -->
    <script>
    (function(){
      let ctx = null;
      const CLICK_VOLUME = 1.0; // ุงุถุจุท ุงููููุฉ ุจูู 0.0 ู 1.0 ููุชุญูู ูู ูุณุชูู ุงูุตูุช (1.0 ุฃุนูู)
      function getCtx(){
        if (!ctx){
          const C = window.AudioContext || window.webkitAudioContext;
          if (!C) return null;
          ctx = new C();
          if (ctx.state === 'suspended') {
            const resume = () => ctx.resume().catch(()=>{}) && document.removeEventListener('click', resume);
            document.addEventListener('click', resume, { once: true });
          }
        }
        return ctx;
      }

      function playClick(){
        const audioCtx = getCtx();
        if (!audioCtx) return;
        const now = audioCtx.currentTime;

        // ุฅูุดุงุก ูุจุถุฉ ูุตูุฑุฉ ุฌุฏุงู (impulse) ูุน ุจุนุถ ุงูุถูุถุงุก ุงูุถุนููุฉ ูุชูุนูู ุงูุตูุช
        const len = Math.max(2, Math.floor(audioCtx.sampleRate * 0.004)); // โ4ms
        const buffer = audioCtx.createBuffer(1, len, audioCtx.sampleRate);
        const data = buffer.getChannelData(0);
        data[0] = 1.0; // ูุจุถุฉ ูููุฉ ุนูุฏ ุงูุจุฏุงูุฉ
        for (let i = 1; i < len; i++) {
          data[i] = (Math.random() * 2 - 1) * Math.exp(-30 * i / len) * 0.25;
        }

        const src = audioCtx.createBufferSource();
        src.buffer = buffer;

        const hp = audioCtx.createBiquadFilter();
        hp.type = 'highpass';
        hp.frequency.value = 1200;

        const g = audioCtx.createGain();
        g.gain.setValueAtTime(0.0001, now);
        g.gain.exponentialRampToValueAtTime(Math.max(0.0001, CLICK_VOLUME), now + 0.001);
        g.gain.exponentialRampToValueAtTime(0.0001, now + 0.02);

        src.connect(hp).connect(g).connect(audioCtx.destination);
        src.start(now);
        src.stop(now + 0.035);
      }

      const CORRECT_VOL = 1.0; // ูุณุชูู ุตูุช ุงููุตุญูุญ (ุชููุฉ)
      const WRONG_VOL = 1.0;   // ูุณุชูู ุตูุช ุงูุฎุทุฃ (ุตูุงุฑุฉ)

      // ุตูุช ุงููุตุญูุญ: ุชููุฉ ูุณุงุจูุฉ ุฃุทูู ูุฃูุซุฑ ูุถูุญุงู (ุณูุณูุฉ ูุบูุงุช ูุตูุฑุฉ)
      function playCorrectChime(){
        const audioCtx = getCtx(); if (!audioCtx) return; const now = audioCtx.currentTime;
        const g = audioCtx.createGain(); g.gain.setValueAtTime(0.0001, now); g.connect(audioCtx.destination);

        // ุณูุณูุฉ ูุบูุชูู ูุน ุชูุงุดู ูุทูู
        const oscA = audioCtx.createOscillator(); oscA.type = 'sine'; oscA.frequency.value = 880;
        const oscB = audioCtx.createOscillator(); oscB.type = 'sine'; oscB.frequency.value = 1100;
        const mix1 = audioCtx.createGain(); mix1.gain.value = 0.6;
        const mix2 = audioCtx.createGain(); mix2.gain.value = 0.6;

        oscA.connect(mix1); oscB.connect(mix2);
        mix1.connect(g); mix2.connect(g);

        // ุชุฒุงูุฏ ุณุฑูุน ุซู ุชูุงุดู ุนูู ูุฏู ~350ms
        g.gain.exponentialRampToValueAtTime(Math.max(0.0001, CORRECT_VOL), now + 0.008);
        g.gain.exponentialRampToValueAtTime(0.0001, now + 0.38);

        oscA.start(now);
        oscA.stop(now + 0.18);
        // ูุบูุฉ ุซุงููุฉ ุชุจุฏุฃ ุจุนุฏ 0.09s ูุชูุชุฌ ูุบูุฉ ุชููุฉ ูุฒุฏูุฌุฉ
        oscB.start(now + 0.09);
        oscB.stop(now + 0.38);
      }

      // ุตูุช ุงูุฎุทุฃ: ุตูุงุฑุฉ ูุตูุฑุฉ-ูุชูุณุทุฉ ุงูุทูู ูุชูุฎูุถ ุจุชุฏุฑุฌ ูุชุดุจู ุงูุตุงูุฑุฉ
      function playWrongBuzzer(){
        const audioCtx = getCtx(); if (!audioCtx) return; const now = audioCtx.currentTime;
        const osc = audioCtx.createOscillator(); osc.type = 'sawtooth'; osc.frequency.setValueAtTime(900, now);
        // ูุฒูู ุชุฑุฏุฏู ุชุฏุฑูุฌู ุนูู ูุฏู ~700ms
        osc.frequency.exponentialRampToValueAtTime(180, now + 0.7);

        const lp = audioCtx.createBiquadFilter(); lp.type = 'lowpass'; lp.frequency.setValueAtTime(1200);
        const g = audioCtx.createGain(); g.gain.setValueAtTime(0.0001, now);
        g.gain.exponentialRampToValueAtTime(Math.max(0.0001, WRONG_VOL), now + 0.02);
        g.gain.exponentialRampToValueAtTime(0.0001, now + 0.75);

        osc.connect(lp).connect(g).connect(audioCtx.destination);
        osc.start(now);
        osc.stop(now + 0.75);
      }

      // ุชุนููู ูุธุงุฆู ูุชููู ูุชุงุญุฉ ููุนุงูู ุงูุฎุงุฑุฌู (ุญุชู ูููู ุงุณุชุฏุนุงุคูุง ูู ุณูุฑุจุช ุงูุฃุณุฆูุฉ)
      window.playCorrectSound = playCorrectChime;
      window.playWrongSound = playWrongBuzzer;

      // ุงุณุชูุงุน ูุฑูุฒู ูุฃู ุถุบุทุฉ ุนูู ุฒุฑ
      document.addEventListener('click', function(e){
        const el = e.target.closest('button, input[type="button"], input[type="submit"], [role="button"], .btn');
        if (el) playClick();
      }, { passive: true });
    })();
    </script>
</body>
